{
  "name": "hull-salesforce",
  "version": "0.4.2",
  "description": "Hull Salesforce integration",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "./node_modules/.bin/babel server -d build --ignore __tests__",
    "start": "./node_modules/.bin/nf start",
    "start:dev": "NODE_ENV=development ./node_modules/.bin/babel-watch -L server",
    "test": "npm run test:lint && jest && npm run test:modules",
    "test:lint": "eslint server",
    "test:modules": "npm outdated --depth=0",
    "clean": "./node_modules/.bin/rimraf build",
    "update": "./node_modules/.bin/updtr",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x",
    "yarn": "0.27.5"
  },
  "jest": {
    "rootDir": "server",
    "transform": {
      ".*": "../node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hull-ships/hull-salesforce.git"
  },
  "keywords": [
    "hull",
    "salesforce",
    "integration"
  ],
  "author": "Hull",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hull-ships/hull-salesforce/issues"
  },
  "homepage": "https://github.com/hull-ships/hull-salesforce",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cache-manager": "^2.4.0",
    "cors": "^2.8.4",
    "ejs": "^2.5.7",
    "express": "^4.15.2",
    "foreman": "^2.0.0",
    "hogan.js": "^3.0.2",
    "hull": "^0.12.2",
    "jsforce": "^1.8.0",
    "librato-node": "5.0.0",
    "lodash": "^4.17.4",
    "newrelic": "^2.0.2",
    "node-babel": "^0.1.2",
    "passport-forcedotcom": "^0.1.4",
    "rimraf": "^2.6.1",
    "winston-logstash": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-watch": "^2.0.7",
    "babel-jest": "^20.0.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4"
  }
}
